

    <section class="hero questions">
        <div class="container">
            <div class="form-wrapper">
                <div class="progressbar">
                    <ul>
                        <li>
                            <div class="svg-wrap">
                                <svg
                                    xmlns="http://www.w3.org/2000/svg"
                                    width="18"
                                    height="18"
                                    viewBox="0 0 18 18"
                                >
                                    <circle
                                        id="Ellipse_3"
                                        data-name="Ellipse 3"
                                        cx="9"
                                        cy="9"
                                        r="9"
                                        fill="#6188df"
                                    />
                                </svg>
                                <p>Vehicles</p>
                            </div>
                        </li>
                        <li>
                            <div class="svg-wrap">
                                <svg
                                    xmlns="http://www.w3.org/2000/svg"
                                    width="18"
                                    height="18"
                                    viewBox="0 0 18 18"
                                >
                                    <circle
                                        id="Ellipse_3"
                                        data-name="Ellipse 3"
                                        cx="9"
                                        cy="9"
                                        r="9"
                                        fill="#6188df"
                                    />
                                </svg>
                                <p>You</p>
                            </div>
                        </li>
                        <li>
                            <div class="svg-wrap">
                                <svg
                                    xmlns="http://www.w3.org/2000/svg"
                                    width="18"
                                    height="18"
                                    viewBox="0 0 18 18"
                                >
                                    <g
                                        id="Ellipse_2"
                                        data-name="Ellipse 2"
                                        fill="none"
                                        stroke="#6188df"
                                        stroke-width="1"
                                    >
                                        <circle
                                            cx="9"
                                            cy="9"
                                            r="9"
                                            stroke="none"
                                        />
                                        <circle
                                            cx="9"
                                            cy="9"
                                            r="8.5"
                                            fill="none"
                                        />
                                    </g>
                                </svg>
                                <p>Compare</p>
                            </div>
                        </li>
                    </ul>
                </div>
                <form action="/form/<%= data.id %>/2" method="POST">
                    <h1 class="form-title">About You</h1>
                    <div class="row">
                        <div
                            class="col-sm-12 col-md-4 form-group input-field"
                        >
                            <input
                                type="text"
                                name="firstName"
                                class="form-control"
                                placeholder="First Name"
                            />
                        </div>
                        <div
                            class="col-sm-12 col-md-4 form-group input-field"
                        >
                            <input
                                type="text"
                                name="lastName"
                                class="form-control"
                                placeholder="Last Name"
                            />
                        </div>
                        <div
                            class="col-sm-12 col-md-4 form-group input-field"
                        >
                            <input
                                type="number"
                                name="phoneNumber"
                                class="form-control"
                                placeholder="Phone Number"
                            />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12 col-md-4 input-field">
                            <select>
                                <option value="" selected disabled>
                                    Gender
                                </option>
                                <option>Male</option>
                                <option>Female</option>
                                <option>Other</option>
                            </select>
                        </div>
                        <div class="col-sm-12 col-md-4 input-field">
                            <select name="status">
                                <option value=""  selected disabled>
                                    Marital Status
                                </option>
                                <option value="married">Married</option>
                                <option value="single">Single</option>
                                
                            </select>
                        </div>
                        <div class="col-sm-12 col-md-4 input-field">
                            <select>
                                <option value="" selected disabled>
                                    Date Of Birth
                                </option>
                                <option>1</option>
                                <option>2</option>
                                <option>3</option>
                                <option>4</option>
                                <option>5</option>
                            </select>
                        </div>
                    </div>

                    <div class="input-field license radio-field">
                        <p>
                            Do you have a valid US Driver's License?
                            <div class="ml-3">
                                <input
                                type="radio"
                                id="yesLicense"
                                name="license1"
                                value="yes"
                            />
                            <label for="yesLicense">Yes</label>
                            </div>
                            <div class="ml-3">
                                <input
                                type="radio"
                                id="noLicense"
                                name="license1"
                                value="no"
                            />
                            <label for="noLicense">No</label>
                            </div>
                        </p>
                    </div>

                    <div id="yesLicenseForm">
                        <div class="row">
                            <div
                                class="col-sm-12 col-md-4 form-group input-field"
                            >
                                <input
                                    type="number"
                                    class="form-control"
                                    placeholder="Age when you got First License"
                                />
                            </div>
                            <div class="col-sm-12 col-md-4 input-field">
                                <select>
                                    <option value="" selected disabled>
                                        Credit Rating
                                    </option>
                                    <option>1</option>
                                    <option>2</option>
                                    <option>3</option>
                                    <option>4</option>
                                    <option>5</option>
                                </select>
                            </div>
                            <div class="col-sm-12 col-md-4 input-field">
                                <select>
                                    <option value="" selected disabled>
                                        Education
                                    </option>
                                    <option>1</option>
                                    <option>2</option>
                                    <option>3</option>
                                    <option>4</option>
                                    <option>5</option>
                                </select>
                            </div>
                            <div class="col-sm-12 col-md-4 input-field">
                                <select>
                                    <option value="" selected disabled>
                                        Occupation
                                    </option>
                                    <option>1</option>
                                    <option>2</option>
                                    <option>3</option>
                                    <option>4</option>
                                    <option>5</option>
                                </select>
                            </div>
                            <div class="col-sm-12 input-field license radio-field">
                                <p>
                                    Has your driver’s license been suspended/revoked in the last 3 years?
                                    <div class="ml-3">
                                        <input
                                        type="radio"
                                        id="yes1"
                                        name="license2"
                                        value="yes"
                                    />
                                    <label for="yes1">Yes</label>
                                    </div>
                                    <div class="ml-3">
                                        <input
                                        type="radio"
                                        id="no1"
                                        name="license2"
                                        value="no"
                                    />
                                    <label for="no1">No</label>
                                    </div>
                                </p>
                            </div>
                            <div class="col-sm-12 input-field license radio-field">
                                <p>
                                    Do you need a SR-22 Financial Responsibility form?
                                    <div class="ml-3">
                                        <input
                                        type="radio"
                                        id="yes2"
                                        name="license3"
                                        value="yes"
                                    />
                                    <label for="yes2">Yes</label>
                                    </div>
                                    <div class="ml-3">
                                        <input
                                        type="radio"
                                        id="no2"
                                        name="license3"
                                        value="no"
                                    />
                                    <label for="no2">No</label>
                                    </div>
                                </p>
                            </div>
                            <div class="col-sm-12 input-field license radio-field">
                                <p>
                                    Add a claim/violation from the past 3 years?
                                    <div class="ml-3">
                                        <input
                                        type="radio"
                                        id="yes3"
                                        name="license4"
                                        value="yes"
                                    />
                                    <label for="yes3">Yes</label>
                                    </div>
                                    <div class="ml-3">
                                        <input
                                        type="radio"
                                        id="no3"
                                        name="license4"
                                        value="no"
                                    />
                                    <label for="no3">No</label>
                                    </div>
                                </p>
                            </div>
                        </div>
                    </div>

                    <div class="text-center mt-5">
                        <button class="custom-btn">
                            Continue<svg
                                xmlns="http://www.w3.org/2000/svg"
                                width="17"
                                height="18.243"
                                viewBox="0 0 17 18.243"
                            >
                                <g
                                    id="Icon_feather-arrow-right"
                                    data-name="Icon feather-arrow-right"
                                    transform="translate(1.5 2.121)"
                                >
                                    <path
                                        id="Path_14"
                                        data-name="Path 14"
                                        d="M7.5,18h14"
                                        transform="translate(-7.5 -11)"
                                        fill="none"
                                        stroke="#fff"
                                        stroke-linecap="round"
                                        stroke-linejoin="round"
                                        stroke-width="3"
                                    />
                                    <path
                                        id="Path_15"
                                        data-name="Path 15"
                                        d="M18,7.5l7,7-7,7"
                                        transform="translate(-11 -7.5)"
                                        fill="none"
                                        stroke="#fff"
                                        stroke-linecap="round"
                                        stroke-linejoin="round"
                                        stroke-width="3"
                                    />
                                </g>
                            </svg>
                        </button>
                    </div>
                </form>
            </div>
        </div>
        <div class="hero__bg">
            <img src="/images/background/herobg.png" alt="" />
        </div>
    </section>

